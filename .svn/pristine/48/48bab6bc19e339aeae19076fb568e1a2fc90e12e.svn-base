
.contTop{
    position: relative;
}
.contTop::before{
    content: "";
    display: block;
    width: 100%;
    height: 100px;
    background-color: #000;
    position: absolute;
    left: 0;
    top: -100px;
}

@media all and (max-width: 991px) {

}

@media all and (min-width: 992px) {
	.contTop::before{
        content: "";
        display: block;
        width: 100%;
        height: 100px;
        background-color: #000;
        position: absolute;
        left: 0;
        top: -100px;
    }
}
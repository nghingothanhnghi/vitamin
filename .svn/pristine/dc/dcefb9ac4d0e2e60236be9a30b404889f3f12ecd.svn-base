/** Basis Modifying for Bootstrap **/

.row {
	margin-left: 0px;
	margin-right: 0px;
	--bs-gutter: 0;		
	padding-left: 0px;
	padding-right: 0px;
}



.row>* {
	padding-left: 0px;
	padding-right: 0px;
}


.column {

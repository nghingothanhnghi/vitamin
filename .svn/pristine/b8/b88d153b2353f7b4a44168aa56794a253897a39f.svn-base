/**/



@media all and (max-width: 991px) {
    #live-broadcast {
        padding: 30px 4%;
    }
}

@media all and (min-width: 992px) {
    #live-broadcast {
        padding: 20px 0;
    }
}


#live-broadcast .heading h2 {
    margin-bottom: .5em;
}


#live-broad .video-wrapper {
    position: relative;
    width: 100%;
    height: 0;
    padding-bottom: 56.25%;
    margin-bottom: 10px;
}

#live-broad .ytp-iframe {
    position: absolute;
    min-width: 100%;
    min-height: 100%;
    right: 0;
    bottom: 0;
}
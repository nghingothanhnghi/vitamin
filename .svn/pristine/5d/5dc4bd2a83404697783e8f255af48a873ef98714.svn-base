/**/




@media all and (min-width: 576px) {
    .submit-btn {
        min-width: 140px;
    }
}



@media all and (max-width: 575px) {
	.searching-result .container.results {
		margin-bottom: 60px;
	}
}


@media all and (min-width: 576px) and (max-width: 767px) {
	.searching-result .container.results {
		margin-bottom: 70px;
	}
}


@media all and (min-width: 768px) and (max-width: 991px) {
	.searching-result .container.results {
		margin-bottom: 80px;
	}
}







@media all and (max-width: 575px) {
	.searching-result .searching-form .submit {
		width: 100%;
	}
	.searching-result .searching-form .submit .submit-btn-wrapper {
		width: 100%;
	}
}




@media all and (max-width: 575px) {
	.searching-result .searching-form .container {
		display: flex;
		flex-wrap: wrap;
	}

	.searching-result .searching-form .date-time {
		flex: 0 0 auto;
		width: 100%;
	}

	.searching-result .searching-form .submit {
		width: 100%;
		flex: 0 0 auto;
		order: 3;
	}
	.searching-result .searching-form .mobile-date-time-filter {
		flex: 0 0 auto;
		width: 100%;
	}	
}



@media all and (min-width: 576px) and (max-width: 991px) {
	.searching-result .searching-form .date-time {
		margin-bottom: 10px;
	}
}


@media all and (min-width: 576px) and (max-width: 991px) {
	.searching-result .searching-form .container {
		display: flex;
		flex-wrap: wrap;
	}

	.searching-result .searching-form .date-time {
		flex: 0 0 auto;
		width: 100%;
	}

	.searching-result .searching-form .submit {
		flex: 0 0 auto;		
		order: 3;
	}

	.searching-result .searching-form .mobile-date-time-filter {
		flex: 1 0 0%;
	}
}















@media (min-width: 768px) and (max-width: 991px) {
    #auto-ship-registration .searching-form .form-row:last-child {
        justify-content: end;
    }
}


/* You can add global styles to this file, and also import other style files */
:root{
  --bs-secondary-color: #525252 !important;
  --bs-dark-rgb:18,18,18 !important;
  --bs-nav-link-font-size: 18px !important;
  --bs-nav-bar-color: #121212 !important;
  --bs-warning-rgb:248,151,27 !important;
  --bs-box-shadow-sm:5px 5px 15px rgba(0,0,0,0.08) !important;
  --base0A: #6f42c1;
  --bd-violet: #712cf9;
  
}
.row.row-cols-2 > *{
  padding-right: calc(var(--bs-gutter-x) * .5);
  padding-left: calc(var(--bs-gutter-x) * .5);
}
.navbar-toggler{
  --bs-navbar-toggler-padding-x:0.35rem !important
}
.navbar-nav .nav-link:hover, .navbar-nav .nav-link.active{ color: #f8971b!important;}
.bg-warning{background-color:#f8971b!important}
.bg-light-orange{background-color:#fffaf4!important}
.bg-light-blue{background-color:#f3f8fe!important}
.bg-darker-light{ background-color: #ebebeb;}
.btn-yellow{ background-color: var(--bs-yellow);}
.btn-yellow:hover{background-color:#e06702;}
.btn-purple{ background-color: var(--base0A);}
.btn-purple:hover{background-color: var(--bd-violet);}
.btn-warning{
  --bs-btn-bg: #f8971b;
  --bs-btn-border-color: #f8971b;
  --bs-btn-hover-bg: #fd7e14;
  --bs-btn-hover-border-color: #ee7612;
  --bs-btn-active-bg: #ee7612;
  --bs-btn-active-border-color: #ee7612;
  --bs-btn-disabled-bg: #f8971b;
  --bs-btn-disabled-border-color: #f8971b;
}
.keepAll {word-break: keep-all;}
.border-dashed{ border-style: dashed !important;}
.rounded-lg{border-radius:16px}
.pt-100{padding-top:100px}
.pb-100{padding-bottom:100px}
.pt-150{padding-top:150px}
.pb-150{padding-bottom:150px}
.py-150{ padding-top: 150px; padding-bottom: 150px;}
.py-100{ padding-top: 100px; padding-bottom: 100px;}
.pb-30{ padding-bottom: 30px;}
.mt-30{ margin-top: 30px;}
.mb-30{ margin-bottom: 30px;}
.thumb-icon-sm{width:132px}
.thumb-icon-sm-svg{width:80px}
.min-h175{ min-height: 175px;}
.hero-header{padding:100px 0;height:calc(100vh - 100px); overflow: hidden;}
/* .hero-header:before{content:"";} */
.btn-arrow-down{position:absolute;background-image:url(./assets/img/narrow.png);background-repeat:no-repeat;background-size:contain;width:60px;height:30px;bottom:15%;left:50%;margin-left:-30px}
.img-bg-64x64{width:221px;height:114px}
.img-bg-200x200{width:200px;height:200px}
.img-bg-400x400{width:400px;height:400px}
.img-bg-506x514{width:506px;height:514px}
.bottom-100{ bottom: -100px !important;}
.bottom-186 {bottom: -186px !important;}
.bottom-260 {bottom: -260px;}
.end-20 {right: 20% !important;}
.end-220 {right: -220px !important;}
.end-230 {right: -230px;}
@media (max-width:400px){
  .max-w-xxs-text-80{ max-width: 40px;font-size: 0.8rem;}
  .me-xxs-1{ margin-right: .25rem !important;}
  .px-xxs-1{ padding-left: .25rem !important; padding-right: .25rem !important;}
}
@media (max-height:578px){
  .hero-header{ padding-top: 20px !important;}
  .navbar ul .menu-item .btn{ font-size: 0.75rem !important;}
}
@media (min-width:1200px) {
  .hero-header .h1,.hero-header h1{font-size:4.25rem!important}
  .section .h2,.section h2{font-size:3.7rem!important}
  .section .h3,.section h3{font-size:2.5rem!important}
  }
  @media (max-width: 575px) {
    .logo-branding-xxs svg{ width: 83px; height: 24px;}
    .lead{font-size: 1rem !important;}
    .crd-img-w70-xs{ max-width: 70% !important;}
    .crd-img-w50-xs{ max-width: 50% !important;}
    .px-xxs-0{padding-left: 0; padding-right: 0;}
    .p-xxs-0{ padding:0;}
    .thumb-icon-xxs-svg{height:43px;width: 34px;}
    .end-360-xxs {right: -360px !important;}
  }
  @media (min-width: 576px) and (max-width:767px) {
    .row.gap-5 .col-sm-4 {width: 26.933% !important;}
    .thumb-icon-sm{width:64px !important}
  }
  @media (min-width: 576px) {
  .row.gap-5 .col-sm-4{width:29.333%}
  .step-payment > li:first-child:before{background-image:url(./assets/img/narrow-right.png) !important;}
  .step-payment > li:last-child:before{background-image:none}
  .step-payment > li::before{top:50%!important;bottom:auto!important;margin-top:-41px!important;right:-54px!important;left:auto!important;transform:none!important}
  .py-sm-150{ padding-top:150px !important; padding-bottom: 150px !important;}
  .pb-sm-150{ padding-bottom: 150px !important;}
  .pt-sm-150 {padding-top: 150px !important;}
  .fs-sm-2{ font-size: 2rem !important;}
  .fs-sm-5{ font-size: 1.25rem !important;}
  .gap-sm-4{ gap: 1.5rem !important;}
  .card-image{min-height:100px;}
  .section-relative-below{margin-top:-300px !important}
  .section-relative-below-content{padding-top:300px !important;}
  }
  @media (min-width: 768px) {
    header{min-height: 100px;}
    .navbar-collapse{ padding: 0 !important;}
  }
 
  /* breakpoint for large screen */
  @media (min-width: 992px) {
    .row-5col .col-lg-2{width:16.967% !important}
    .card-image{padding:20px !important;}
    .section-relative-below{margin-top:-90px !important}
    .section-relative-below-content{padding-top:80px !important;}  
  }
  .row-5col .col-2{width:28.967%}
  .navbar-collapse{padding: 30px 0 10px;}
  .navbar-collapse .nav-link{ padding: 10px;}
.devide{width:40px;height:2px;display:block;background-color:#f9971a;margin-top:20px;margin-bottom:20px}
.list-hover-icon > li:last-child .card-image img{height:75px}
.card-hover:hover,.nav-link.card-hover.active{background-color:#f9971a;color:#fff}
.card-hover:hover svg g path,.nav-link.card-hover.active svg g path{fill:#fff!important}
.card-image{padding:10px;position:relative}
.card-image img{width:auto;max-height:100px;position:absolute;left:50%;top:50%;object-fit:contain;margin-left:-30px;margin-top:-30px}
.section-relative-top,.section-relative-below{position:relative}
.section-relative-top{z-index:1}
.section-relative-below{margin-bottom:-90px;margin-top: -235px;}
.section-relative-below-content{padding-top: 200px;padding-bottom:20px}
.step-payment > li{position:relative}
.step-payment > li::before{transform:rotate(270deg);bottom:-25px;left:50%;margin-left:-33px;content:"";position:absolute;background-image:url(./assets/img/narrow-right.png);background-repeat:no-repeat;background-size:contain;width:60px;height:25px;z-index:1}
.step-payment > li:first-child:before{background-image:none}
.rotate-360{transform-origin:center;-webkit-animation:spin 80s linear infinite;-moz-animation:spin 80s linear infinite;animation:spin 80s linear infinite}
@-moz-keyframes spin {
100%{-moz-transform:rotate(360deg)}
}
@-webkit-keyframes spin {
100%{-webkit-transform:rotate(360deg)}
}
@keyframes spin {
0%{transform:rotate(0deg)}
100%{transform:rotate(360deg)}
}

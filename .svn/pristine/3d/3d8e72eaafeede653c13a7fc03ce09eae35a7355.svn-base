/**/


.page-menu {
    background: #FFF;
    box-shadow: 0px 5px 15px rgba(0,0,0,.1); 
}

ul {
    margin: 0;
}

ul li a {
    color: #584E3F;
    display: block;
    padding: 15px 0;
    transition: all ease-in-out .3s;
}

ul li a:hover,
ul li.active a {
    color: #f8971b;
}

ul li:not(:last-child) a {
    margin-right: 30px;
}




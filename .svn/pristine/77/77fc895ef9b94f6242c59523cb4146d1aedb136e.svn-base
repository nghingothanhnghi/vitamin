.bg-warning{
    background-color: #F9971A !important;
}
.bg-light-orange{
    background-color: #FFFAF4 !important;
}
.bg-light-blue{
    background-color: #F3F8FE !important;
}
.rounded-lg{
    border-radius: 16px;
}
.thumb-icon-sm{
    width: 100px;
}
.thumb-icon-sm-svg{
    width: 80px;
}
.hero-header{
    padding: 100px 0;
    min-height: 600px;
}
.img-bg-64x64{
    width: 221px;
    height:114px
}
.end-20{
    right: 20% !important;
}
@media (min-width: 576px) {
    .row-5col .col-sm-2{
        width: 17.967%;
    }
    .row.gap-5 .col-sm-4{
        width:29.333%;
    }

}
.list-hover-icon > li:last-child .card-image img{ height: 75px;}
.card-hover:hover{ background-color:#F9971A ;}
.card-hover:hover svg g path {fill: #fff !important;}
.card-image {min-height: 100px; padding: 20px; position: relative;}
.card-image img{ width: auto; max-height: 100px; position: absolute; left:50%; top:50%; object-fit: contain;margin-left: -30px;margin-top: -30px;}
.section-relative-top,.section-relative-below{position: relative;}
.section-relative-top{z-index: 1;}
.section-relative-below{margin-bottom: -90px;margin-top: -90px;}
.section-relative-below-content{ padding-top: 80px;padding-bottom: 100px;}
.loading-box{
	top: 0;
    left: 0;
    right: 0;
    bottom: 0;
	z-index: 1999;
	position: fixed;
	border-radius: 0px;
    background-color: rgba(0, 0, 0, 0.3);
}

/* loader */


.full-screen {
    position: fixed;
    position: -ms-page;
}
.spinner-three-bounce {
    width: 70px;
    text-align: center;
    position: absolute;
    top: 0;
    left: 0;
    right: 0;
    bottom: 0;
    height: 20px;
    margin: auto;
    z-index: 2000;


}
.spinner-three-bounce  > div {
	width: 18px;
    height: 18px;
    background-color: #fff;
    border-radius: 100%;
    display: inline-block;
    animation: sk-bouncedelay 1.4s infinite ease-in-out both;
}

.spinner-three-bounce .bounce1  {
    animation-delay: -.32s;
}

.spinner-three-bounce .bounce2  {
    animation-delay: -.16s;
}


/* end loader */

  .hidden-box{
	display: none;
  }



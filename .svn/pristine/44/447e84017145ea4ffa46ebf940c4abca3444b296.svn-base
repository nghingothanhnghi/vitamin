


@media all and (max-width: 991px) {
    #business-info .main-text {
        padding: 30px 4%;
        margin-bottom: 25px;
    }
}


@media all and (min-width: 992px) {
    #business-info .main-text {
        padding: 35px 4%;
        margin-bottom: 30px;
    }
}


#business-info .main-text .title h2 {
    font-weight: bold;
    margin-bottom: 10px;
}


#business-info .main-text .content {    
    margin-bottom: 40px;
}




#business-info .main-text .medals .medal {    
    text-align: center;
    box-shadow: 0px 5px 15px rgba(0,0,0,.05);
    transition: all ease-in-out .3s;
    background: #FFF;
}

#business-info .main-text .medals .medal:hover {   
    box-shadow: 0px 5px 15px rgba(0,0,0,.15);
}




@media all and (max-width: 991px) {
	#business-info .main-text .medals .medal {   
        padding: 25px 4%;
    }
}


@media all and (min-width: 992px) {	
    #business-info .main-text .medals .medal {   
        padding: 30px 4%;
    }
}









@media all and (max-width: 575px) {
    #business-info .main-text .medals .medal {   
        width: calc(50% - 5px); 
        margin-bottom: 10px;
    }

    #business-info .main-text .medals .medal:not(:nth-child(2n)) {   
        margin-right: 10px;
    }
}



@media all and (min-width: 576px) {
    #business-info .main-text .medals .medal {    
        margin-right: 2%;
    }
}






#business-info .main-text .medals .medal img {    
    width: auto;
    display: block;
    margin-left: auto;
    margin-right: auto;
    margin-bottom: 20px;
}

#business-info .main-text .medals .medal p {
    margin-left: auto;
    margin-right: auto;
    margin-bottom: 0;
}



.bgr {
    background: #FBFEF6;
    position: relative;
}





